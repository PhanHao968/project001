  "scripts": {
    "start": "nodemon --inspect src/index.js",
    "build:tailwind": "tailwind build -i ./src/public/css/index.css -o ./src/public/css/tailwind.css",
    "build:fontawesome": "mkdir -p public/vendor/@fortawesome/fontawesome-free/webfonts && mkdir -p public/vendor/@fortawesome/fontawesome-free/css && cp -a ./node_modules/@fortawesome/fontawesome-free/webfonts public/vendor/@fortawesome/fontawesome-free/ && cp ./node_modules/@fortawesome/fontawesome-free/css/all.min.css public/vendor/@fortawesome/fontawesome-free/css/all.min.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run build:fontawesome && npm start"
  },
